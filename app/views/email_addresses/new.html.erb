<h1> <%= "New Email Address for #{@email_address.contact}" %>  </h1>

<%= render 'form', email_address: @email_address %>

<% if @email_address.contact_type == 'Person'%>
<%= link_to 'Back', person_path(@email_address.contact) %>
<% elsif %>
<%= link_to 'Back', company_path(@email_address.contact
  ) %>
<% end %>
