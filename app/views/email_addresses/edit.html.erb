<h1> <%= "Editing email address for #{@email_address.contact.name}" %> </h1>

<%= render 'form', email_address: @email_address %>

<%= link_to 'Show', @email_address %> |
<% if @email_address.contact_type == 'Person'%>
<%= link_to 'Back', person_path(@email_address.contact_id) %>
<% elsif %>
<%= link_to 'Back', company_path(@email_address.contact_id) %>
<% end %>
